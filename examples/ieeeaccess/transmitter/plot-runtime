set terminal postscript eps enhanced color font 'Helvetica,34'

set font 'Helvetica,34'

#set xtics 0,2,36
set xtics 0,10,60
set xrange [0:60]


set xtics font "Helvetica,34" 
set ytics font "Helvetica,34" 
set key spacing 1.3 font 'Helvetica,26'

set ylabel 'Runtime (microsec.)'  font 'Helvetica,34'
set xlabel "Number of Subcarriers N (k=N/2)"  font 'Helvetica, 34'


plot 'runtime-adapted-vs-combinadic-data.txt' using 1:4:5  with errorlines lc -1 pt 7 dt 2 ps 1.2 t "OFDM-IM Mapper",\
     'runtime-adapted-vs-combinadic-data.txt' using 1:2:3 with errorlines  lc -1 lt 1  pt 5 ps 1.2 t"Proposed Mapper"


set output 'runtime.eps'
replot
set term pop; set out;
