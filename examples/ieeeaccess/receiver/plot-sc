set terminal postscript eps enhanced color font 'Helvetica,15.5'

set font 'Helvetica,15.5'

#set xtics 0,2,36
set xtics 0,4,62   

set xtics font "Helvetica,15.5" 
set ytics font "Helvetica,15.5" 
set key spacing 1.3 font 'Helvetica,15.5'

set ylabel 'Throughput (Mbps)'  font 'Helvetica,18'
set xlabel "Number of Subcarriers N (k=N/2)"  font 'Helvetica, 18'


plot 'scthroughput-adapted-vs-combinadic-data.txt' using 1:4:5  with errorlines lt 0 lw 2.5 t"Original IxS",\
     'scthroughput-adapted-vs-combinadic-data.txt' using 1:2:3 with errorlines  lt -1 lw 1 t"Adapted IxS"


set output 'sc.eps'
replot
set term pop; set out;
